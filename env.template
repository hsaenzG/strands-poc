# Environment Variables for Harry Potter Knowledge Base Integration
# Copy this file to .env and update with your actual values

# AWS Configuration
REGION_NAME=us-east-1
MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# Amazon Knowledge Base Configuration
# Replace these placeholder values with your actual Knowledge Base IDs
KNOWLEDGE_BASE_ID=YOUR_KNOWLEDGE_BASE_ID_HERE
KNOWLEDGE_BASE_DATA_SOURCE_ID=YOUR_DATA_SOURCE_ID_HERE

# AWS Profile (for local development)
AWS_PROFILE=dev-personal

# Optional: Lambda Configuration
LAMBDA_TIMEOUT=30
LAMBDA_MEMORY=512

# Optional: API Configuration
CORS_ORIGIN=*
API_STAGE=prod

# Optional: Logging
LOG_LEVEL=INFO
